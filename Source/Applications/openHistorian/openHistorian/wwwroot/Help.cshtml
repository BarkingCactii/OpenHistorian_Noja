@using openHistorian.Model
@using RazorEngine.Templating
@inherits TemplateBase<AppModel>
@{ 
    Layout = "Layout.cshtml";
    ViewBag.Title = "Help";
}
<table class="table table-striped">
    <colgroup>
        <col class="col-md-12">
    </colgroup>
    <tbody>
        <tr>
            <td>
                <p class="image-wrap">
                    <div><a id="Roles"></a><i><b>User Roles:</b></i></div>
                    <div>
                        <b>Administrator</b> Information Technology personnel authorized to establish users and roles and
                        set application specific configuration parameters.
                    </div>
                    <div>
                        <b>Editor</b> Professional staff within the charged with making maintaining common system configuration.
                    </div>
                    <div>
                        <b>Viewer</b> - Users that can view the status of the system patches but cannot make any changes within the system.
                    </div>
                </p>
            </td>
        </tr>
        <tr style="height: 150px; border: none">
            <td>&nbsp;</td>
        </tr>
    </tbody>
</table>
